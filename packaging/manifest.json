      "Optional": true
    },
    {
      "Source": "assets/sublime_text_build_4200_x64.rar",
      "Destination": "tools/sublime_text_build_4200_x64.rar",
      "Description": "Sublime Text Editor",
      "Optional": true
    },
    {
      "Source": "assets/scripts",
      "Destination": "scripts",
      "Description": "PowerShell Scripts for Remediation and OS Access"
    },
    {
      "Source": "assets/IRNazanin.ttf",
      "Destination": "fonts/IRNazanin.ttf",
