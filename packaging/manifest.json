    {
      "Source": "config/audit_config.example.jsonc",
      "Destination": "config/audit_config.example.jsonc",
      "Description": "Audit Config Template (with comments)"
    },
    {
      "Source": "config/credentials.example.jsonc",
      "Destination": "config/credentials.example.jsonc",
      "Description": "Credentials Template (with comments)"
    },
    {
      "Source": "config/setup_config.bat",
      "Destination": "config/setup_config.bat",
      "Description": "Generate production JSON from JSONC templates"
    },
    {
      "Source": "docs/USER_GUIDE_DIST.md",
      "Destination": "UserGuide.md",
      "Description": "User Guide"
    },
    {
      "Source": "resources",
      "Destination": "resources",
